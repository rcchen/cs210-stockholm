<div class="container">

	<h3><%= @worksheet.title %></h3>
	<%= link_to 'Edit', {controller: 'worksheet', action: 'edit'}, class: 'btn btn-success btn-or' %>

	<% @worksheet.visualizations.each do |visualization| %>
	<div class="visualization">
		<iframe src="/visualization/<%= visualization.identifier %>" width="100%" height="500px" frameBorder="0" scrolling="no"></iframe>
	</div>
	<% end %>

</div>