<div class="container">

	<h3><%= @worksheet.title %></h3>
	<%= link_to 'Edit', {controller: 'worksheet', action: 'edit'}, class: 'btn btn-success btn-or' %>

	<% @worksheet.visualizations.each do |visualization| %>
	<div class="visualization" id="<%= visualization.identifier %>" style="width: 940px; height: 500px; margin: 0 auto;">
		<iframe src="/visualization/<%= visualization.identifier %>/view" frameBorder="0"></iframe>
	</div>
	<% end %>

</div>