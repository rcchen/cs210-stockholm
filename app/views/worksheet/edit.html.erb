<div class="container">
	<div id="edit">
		<h3>Edit Worksheet</h3>
		<table class="table table-hover">
		<thead>
			<tr>
				<th>Identifier</th>
				<th>View</th>
				<th>Destroy</th>
			</tr>
		</thead>
		<tbody>
		<% @worksheet.visualizations.each do |visualization| %>
			<tr>
				<td><%= visualization.identifier %></td>
				<td><a href="/visualization/<%= visualization.identifier %>">View</a></td>
				<td><a href="/visualization/<%= visualization.identifier %>/destroy">Destroy</a></td>
			</tr>
		<% end %>
		</tbody>
		</table>
		<%= link_to image_tag("buttons/newvis.png", {:id=> "create-visualization", :class => "img-rounded"}), {controller: 'visualization', action: 'create', worksheet: params[:id]} %>
	</div>
</div>